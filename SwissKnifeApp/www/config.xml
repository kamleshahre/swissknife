<?xml version='1.0' encoding='utf-8'?>
<widget id="com.nmdadiii.swissknife" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SwissKnife</name>
    <description>
        The best application for your festivals.
    </description>
    <author email="nico.verb@gmail.com" href="www.nicoverbruggen.be">
        Nico Verbruggen
    </author>
    <content src="index.html" />
